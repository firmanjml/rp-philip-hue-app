{
  "expo": {
    "scheme": "lighue",
    "name": "lighue",
    "slug": "lighue",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashscreenwhite.png",
    "androidStatusBar" : {
      "barStyle" : "light-content",
      "backgroundColor" : "#143954"
    },
    "splash": {
      "image": "./assets/images/splashscreenwhite.png",
      "resizeMode": "contain",
      "backgroundColor": "#143954"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.rp.lighue"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rp.lighue",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses FaceID to authenticate users for usage on this app."
      }
    }
  }
}
